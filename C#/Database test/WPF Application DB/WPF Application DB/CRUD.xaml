<Window x:Class="WPF_Application_DB.CRUD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Application_DB"
        mc:Ignorable="d"
        Title="CRUD" Height="207" Width="800">
    <Grid Height="225" VerticalAlignment="Top">
        <TextBox Name="sName" HorizontalAlignment="Left" Margin="38,21,0,0" TextWrapping="Wrap" Text="name" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="sId" HorizontalAlignment="Left" Margin="163,21,0,0" TextWrapping="Wrap" Text="id" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="sAge" HorizontalAlignment="Left" Margin="288,21,0,0" TextWrapping="Wrap" Text="age" VerticalAlignment="Top" Width="120"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="468,21,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.019,-0.146" Click="Button_Click"/>
    </Grid>
</Window>
